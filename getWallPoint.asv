function wallpoint = getWallPoint(img,rayinfo)
%% 
raystep = 3; % pixels
raynum = 20;
xc = rayinfo(3);
yc = rayinfo(4);
alpha = rayinfo(2);
%x = zeros(raynum,1);
%y = zeros(raynum,1);
for i=1:raynum
    x = raystep*i*cos(alpha)+xc;
    y = -(raystep*i*sin(alpha)+yc); % minus sign b/c of image coordinate
    
end

end
